           00C : 9842;  -- 	OUT    Note
           00D : B802;  -- 	LOADI  2	
           00E : 9840;  -- 	OUT    Mode
           00F : B802;  -- 	LOADI  2
           010 : 9843;  -- 	OUT	   Volume
           011 : 8017;  -- 	CALL   Delay
           012 : 8017;  -- 	CALL   Delay
           013 : 8017;  -- 	CALL   Delay
           014 : 8017;  -- 	CALL   Delay
           015 : 8017;  -- 	CALL   Delay
           016 : 2800;  -- 	JUMP   0
